---
import Layout from '../layouts/BaseLayout.astro';
import Contact from '../components/contact/Feedback.astro';
---

<Layout>

<section class="bg-white dark:bg-gray-900">
  <div class="py-8 lg:py-16 px-4 mx-auto max-w-screen-md">
      <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-center text-gray-900 dark:text-white">
        Have Some Feedback?</h2>
      <p class="mb-8 lg:mb-16 font-light text-center text-gray-500 dark:text-gray-100 sm:text-xl">
        Your feedback helps users and businesses alike and is greatly appreciated.</p>
<Contact />
      </div>
</section>
</Layout>